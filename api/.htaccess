
RewriteEngine On

RewriteRule    ^v([A-Za-z0-9-.]+)/([A-Za-z0-9-]+)?$        	            $2.php?version=v$1    [QSA,L]
RewriteRule    ^v([A-Za-z0-9-.]+)/([A-Za-z0-9-]+)/([0-9-]+)$        	$2.php?version=v$1&num=$3&act=getDetail    [L,QSA]
RewriteRule    ^v([A-Za-z0-9-.]+)/([A-Za-z0-9-]+)/([A-Za-z0-9-]+)$      $2.php?version=v$1&tipo=$3&act=list    [L,QSA]



ErrorDocument 404 /error404.php

